//1
<html>
  <body>
    <h1>Заголовок:</h1>
    <ul>
      <li>JavaScript</li>
      <li>CSS</li>
      <li>Html</li>
      </ul>
      </body>
      </html>
      
     
//H1
let textH1 = document.querySelector("h1").innerHTML;
console.log(textH1)

//UL
let textH1 = document.querySelector("ul").innerHTML;
console.log(textH1)

//LI
let textH1 = document.querySelector("li").innerHTML;
console.log(textH1)

